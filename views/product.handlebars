<!-- featured product -->
  <div class="row">
    <div class="col s6 featured-product-image">
      {{#each page.featuredArt}}
        {{> featured_card img=src title=title description=description id=id}}
      {{/each}}
    </div>
  <!-- end of featured product -->
    <div class="col s6">
      <div class="featured-description-box">
        {{#each page.artistLife}}
          {{> artistBio_card img=src title=title}}
        {{/each}}
      </div>
    </div>
  </div>

  <div class="divider"></div>
  <div class="section">
    <div class="row">
      <h5>Additional Works by this Artist</h5>
      {{#each page.artImages}}
        {{> artwork_card img=src title=title id=id}}
      {{/each}}
    </div>
  </div>
</div>

